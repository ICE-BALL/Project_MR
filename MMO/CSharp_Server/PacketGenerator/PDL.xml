<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	<packet name="PlayerList">
		<list name="player">
			<int name="PlayerId"/>
			<int name="Map_Zone"/>
			<bool name="IsSelf"/>
			<float name="PosX"/>
			<float name="PosY"/>
			<float name="PosZ"/>
			<float name="RotX"/>
			<float name="RotY"/>
			<float name="RotZ"/>
		</list>
	</packet>
	<packet name="BroadCastEnter">
		<int name="PlayerId"/>
		<int name="Map_Zone"/>
		<float name="PosX"/>
		<float name="PosY"/>
		<float name="PosZ"/>
		<float name="RotX"/>
		<float name="RotY"/>
		<float name="RotZ"/>
	</packet>
	<packet name="BroadCastLeave">
		<int name="PlayerId"/>
		<int name="Map_Zone"/>
	</packet>
	<packet name="PlayerMove">
		<int name="PlayerId"/>
		<int name="StateConvertNum"/>
		<int name="Map_Zone"/>
		<float name="PosX"/>
		<float name="PosY"/>
		<float name="PosZ"/>
		<float name="RotX"/>
		<float name="RotY"/>
		<float name="RotZ"/>
	</packet>
	<packet name="Data">
		<int name="PlayerId"/>
		<int name="Map_Zone"/>
		<int name="Level"/>
		<int name="MaxHp"/>
		<float name ="Hp"/>
		<int name="MaxMp"/>
		<float name ="Mp"/>
		<float name="Attack"/>
		<float name="AttackSpeed"/>
		<float name="Speed"/>
	</packet>
	<packet name="MonsterList">
		<list name="monsters">
			<int name="MonsterId"/>
			<int name="Map_Zone"/>
			<float name="PosX"/>
			<float name="PosY"/>
			<float name="PosZ"/>
			<float name="RotX"/>
			<float name="RotY"/>
			<float name="RotZ"/>
		</list>
	</packet>
	<packet name="MonsterData">
		<int name="MonsterId"/>
		<int name="Map_Zone"/>
		<int name="Level"/>
		<int name="MaxHp"/>
		<float name ="Hp"/>
		<int name="MaxMp"/>
		<float name ="Mp"/>
		<float name="Attack"/>
		<float name="AttackSpeed"/>
		<float name="Speed"/>
	</packet>
</PDL>
